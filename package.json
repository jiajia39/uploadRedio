{
  "name": "express-starter",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "nodemon src/server.js --exec babel-node --ignore **/__tests__/** --ignore **/__mocks__/**",
    "build": "babel src -d dist --ignore **/__tests__/**,**/__mocks__/**",
    "serve:dev": "env-cmd -f ./envs/dev.js yarn serve",
    "serve:prod": "env-cmd -f ./envs/prod.js yarn serve",
    "build:dev": "env-cmd -f ./envs/dev.js yarn build",
    "build:prod": "env-cmd -f ./envs/prod.js yarn build",
    "lint": "eslint 'src/**/*.js'",
    "unit": "jest --coverage",
    "preview": "caddy run",
    "setup": "cd e2e && yarn install",
    "e2e": "cd e2e && yarn test",
    "meas": "cd e2e && yarn perf",
    "active": "cd mock/requests && yarn install",
    "mock": "cd mock/requests && yarn serve",
    "_codecov": "codecov",
    "_heroku": "heroku"
  },
  "pkg": {
    "assets": [
      "./*.js",
      "./dist/**/*",
      "./prisma/*",
      "./node_modules/**/*"
    ]
  },
  "bin": "./dist/server.js",
  "dependencies": {
    "@influxdata/influxdb-client": "^1.29.0",
    "@influxdata/influxdb-client-apis": "^1.31.0",
    "@mrbatista/passport-apple-token": "^1.0.0",
    "@nicokaiser/passport-apple": "^0.2.1",
    "@prisma/client": "^4.4.0",
    "@sentry/node": "5.12.4",
    "@small-tech/express-ws": "^1.0.1",
    "@smth-for/passport-google-access-token": "^1.0.3",
    "axios": "^1.1.3",
    "bcrypt": "5.0.0",
    "chalk": "2.4.2",
    "cloudinary": "1.18.1",
    "compression": "1.7.4",
    "connect-mongo": "^3.2.0",
    "cors": "2.8.5",
    "decimal.js": "^10.4.2",
    "express": "4.17.1",
    "express-jwt": "^7.7.2",
    "express-rate-limit": "5.0.0",
    "express-session": "1.17.0",
    "express-swagger-generator": "^1.1.17",
    "fs": "^0.0.1-security",
    "helmet": "3.21.2",
    "influx": "^5.0.0-alpha.4",
    "jsonwebtoken": "8.5.1",
    "keycloak-connect": "^18.0.0",
    "lodash": "4.17.21",
    "mongoose": "5.11.8",
    "mongoose-serial": "^1.0.4",
    "morgan": "1.9.1",
    "multer": "1.4.2",
    "multer-storage-cloudinary": "^4.0.0",
    "node-cron": "^3.0.2",
    "node-xlsx": "^0.21.0",
    "otplib": "^12.0.1",
    "passport": "0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-facebook-token": "^4.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "4.0.0",
    "pm2": "3.5.1",
    "swagger-jsdoc": "^6.2.1",
    "swagger-ui-express": "^4.4.0",
    "sync-request": "^6.1.0",
    "ua-parser-js": "^1.0.2",
    "uuid": "^9.0.0",
    "winston": "3.2.1",
    "ws": "^7.4.1",
    "xlsx-style": "^0.8.13"
  },
  "devDependencies": {
    "@babel/cli": "7.12.10",
    "@babel/core": "7.12.10",
    "@babel/eslint-parser": "^7.14.3",
    "@babel/node": "7.12.10",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/preset-env": "7.12.10",
    "babel-jest": "24.8.0",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-root-import": "6.6.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "babel-plugin-transform-node-env-inline": "^0.4.3",
    "codecov": "3.8.1",
    "cross-env": "^5.2.0",
    "env-cmd": "^10.1.0",
    "eslint": "6.3.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.1.0",
    "eslint-import-resolver-babel-plugin-root-import": "1.1.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "3.1.0",
    "faker": "4.1.0",
    "heroku": "7.42.1",
    "jest": "24.8.0",
    "nodemon": "1.19.1",
    "prettier": "1.18.2",
    "prisma": "^4.4.0"
  }
